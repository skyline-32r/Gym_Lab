<%- include("parts/html-head") %>
    <%- include("parts/navbar") %>
        <div class="container">
            <table class="table table-striped table-border">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Age</th>
                    </tr>
                </thead>
                <tbody>
                    <% for (let sale of sales) { %>
                        <tr>
                            <td>
                                <%= sale.id %>
                            </td>
                            <td>
                                <%= sale.name %>
                            </td>
                            <td>
                                <%= sale.age %>
                            </td>
                        </tr>
                        <% } %>
                </tbody>
            </table>
        </div>
        <%- include("parts/scripts") %>
            <%- include("parts/html-footer") %>